{
  "welcomeFile": "/ui/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "/odata/v2",
      "authenticationType": "none",
      "destination": "srv_api",
      "csrfProtection": false
    },
    {
      "source": "^/ui/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}